{
    "root": "", 
    "route": "/",
    "version": { 
      "type": "package",
      "files": [ "public/index.html" ]
    },

    "css": {
        "paths": [
            "css"
        ], 
        "target": "public/application.css"
    },

    "js": {
        "identifier": "require", 
        "libs": [
            "lib"
        ], 
        "modules": [
            "jqueryify", 
            "spine", 
            "spine/lib/local", 
            "spine/lib/list", 
            "spine/lib/ajax", 
            "spine/lib/route", 
            "spine/lib/manager", 
            "spine/lib/relation"
        ], 
        "paths": [
            "app"
        ], 
        "target": "public/application.js"
    },

    "static": {
        "/": "public", 
        "/test": "test/public"
    },

    "test": {
        "identifier": "specs",
        "libs": [],
        "paths": [
            "test/specs"
        ], 
        "target": "test/public/specs.js",
        "type": "jasmine",
        "runner": "karma"
    }
}

